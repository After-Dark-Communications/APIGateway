{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/Values",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/values",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "Swaggerkey": "values"
    },
    {
      "DownstreamPathTemplate": "/api/surveys",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 5002
        }
      ],
      "UpstreamPathTemplate": "/surveys",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "Swaggerkey": "surveys"
    }
  ],

  "SwaggerEndPoints": [
    {
      "Key": "values",
      "Config": [
        {
          "Name": "Test API",
          "Version": "v1",
          "Url": "http://host.docker.internal:5001/swagger/v1/swagger.json"

        }
      ]
    },
    {
      "Key": "surveys"
      "Config": [
        {
          "Name": "surveys API",
          "Version": "v1",
          "Url": "http://host.docker.internal:5002/api/docs/swagger.json"
        }
      ]
    }
  ],
    "GlobalConfiguration": {
        "BaseUrl": "https://localhost:5000"
    }
}